{
    "manifest_version": 2,

    "name": "chrome_substitution",
    "version": "0.1",
    "permissions": [
        "*://*/*", "background"
    ],
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["jquery.min.js", "main.js"],
          "run_at": "document_idle",
          "all_frames": true
        }
    ]
}
